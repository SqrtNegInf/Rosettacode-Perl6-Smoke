sub propdivsum (\x) {
    my @l = x > 1, gather for 2 .. x.sqrt.floor -> \d {
        my \y = x div d;
        if y Ã— d == x { take d; take y unless y == d }
    }
    [+] gather @l.deepmap(*.take);
}

say my @out = sort bag map { propdivsum($_) <=> $_ }, 1..20000;

