#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/FTP
#c# 2016-06-01 <RC, 2018-04-25 <RC
#m# MOAR: OK
#j# JVM:  BROKEN

die 'ecosystem...' if $*VM ~~ /jvm/;  # wait for zef-j

# Works with: rakudo version 2018.04

use Net::FTP;
 
my $ftp = Net::FTP.new(:host('mirrors.sohu.com'), :passive);
 
$ftp.login();
 
my @res.push: $_<name> for $ftp.ls();
.say for @res;
 
#$ftp.get( 'index.html', :binary );

use Test;
ok set('FreeBSD', 'OpenBSD', 'debian', 'centos') âŠ† @res.SetHash;
