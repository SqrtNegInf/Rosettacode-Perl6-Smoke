#n# runs and does nothing (with no args)

#!/usr/local/bin/perl6
use MONKEY; EVAL '(exit $?0)' && EVAL 'exec perl6 $0 ${1+"$@"}'
& EVAL 'exec perl6 $0 $argv:q'
        if 0;

print "ok 1\n";
