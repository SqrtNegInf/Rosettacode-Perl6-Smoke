#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/Modular_arithmetic
#c# 2015-11-22 <RC
#m# MOAR: OK
#j#  JVM: OK

# There is a module called Modular which works basically as Perl 5's Math::ModInt.

use Modular;
sub f(\x) { x**100 + x + 1};
say f( 10 Mod 13 );

use Test;
is f( 10 Mod 13), 1, 'Bingo!';
