#!/usr/bin/env raku
#u# http://rosettacode.org/wiki/Find_first_missing_positive
#c# 2021-02-23 <RC
#m# MOAR: OK
#j# JVM:  OK

my @res;

@res.push: $_.gist ~ ' ==> '~ (1..Inf).first( -> \n { n âˆ‰ $_ } ) for
[ 1, 2, 0], [3, 4, 1, 1], [7, 8, 9, 11, 12], [1, 2, 3, 4, 5], [-6, -5, -2, -1], [5, -5], [-2], [1], [];

.say for @res;

my $ref = q:to/END/;
[1 2 0] ==> 3
[3 4 1 1] ==> 2
[7 8 9 11 12] ==> 1
[1 2 3 4 5] ==> 6
[-6 -5 -2 -1] ==> 1
[5 -5] ==> 1
[-2] ==> 1
[1] ==> 2
[] ==> 1
END

use Test;
is @res.join("\n"), chomp $ref;
