#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/Polymorphic_copy
#c# 2018-03-21 <RC
#m# MOAR: OK
#j# JVM:  OK
#n# odd: extra curlies?

my Cool $x = 22/7 but role Fink { method brag { return "I'm a cool {self.WHAT.perl}!" }}
my Cool $y = $x.clone;
say my $result = $y.brag;

use Test;
is $result, Q <I'm a cool Rat+{Fink}!>;
