#!/usr/bin/env raku
#u# http://rosettacode.org/wiki/Smallest_square_that_begins_with_n
#c# 2021-03-19 <RC
#m# MOAR: OK
#j# JVM:  OK
#n# minor display tweaks

my @res;

my @needles  = (1..49);
my @haystack = (1..*) ZÃ— (1..*);
for @needles -> \needle {
   for @haystack -> \hay {
      { @res.push: needle ~ ' => ' ~ sqrt(hay) ~ ' => ' ~ hay and last } if hay.starts-with: needle
      #{ say needle ~ ' => ' ~ hay and last } if hay.starts-with: needle
   }
}

#.say for @res;
.say for @res[0..9, *-1];

my $ref = q:to/END/;
