my @res;

sub div-count (\x) {
    return 2 if x.is-prime;
    flat (1 .. x.sqrt.floor).map: -> \d {  # DH '+' not needed?
        unless x % d { my \y = x div d; y == d ?? y !! (y, d) }
    }
}

my $limit = 20;

my @primes = grep { .is-prime }, 1..*;
@primes[$limit]; # prime the array. SCNR

@res.push: "First $limit terms of OEIS:A073916";
my @terms = gather for 1..$limit -> $n {
    ($n > 4 and $n.is-prime) ??
    take exp($n - 1, @primes[$n - 1]) !!
    do {
        my $i = 0;
        # with JVM, can't collect terms out of iterator? [MOAR works]
        my $iterator = $n %% 2 ?? (1..*) !! (1..*).map: *Â²;
        take $iterator.first: {
            next unless $n == .&div-count;
            next unless ++$i == $n;
            $_
        }
    }
};
@res.push: join ' ', @terms;

.say for @res;

my $ref = q:to/END/;
