#!/usr/bin/env perl6
#u# http://rosettacode.org/wiki/User_input/Text
#c# 2016-01-31 <RC
#m# MOAR: OK
#j#  JVM: OK
#=# RC pipe: ad astra, per asperagus\n1024

my $str = prompt("Enter a string: ");
my $int = prompt("Enter a integer: ");
say "\nString: $str\nInteger: $int";

use Test;
is $str, 'ad astra, per asperagus';
is $int, 1024;
